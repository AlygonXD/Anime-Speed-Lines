				float2 CenteredUV15_g7 = ( IN.ase_texcoord3.xy - float2( 0.5,0.5 ) );
				float2 break17_g7 = CenteredUV15_g7;
				float2 appendResult23_g7 = (float2(( length( CenteredUV15_g7 ) * _SpeedLinesRadialScale * 2.0 ) , ( atan2( break17_g7.x , break17_g7.y ) * ( 1.0 / TWO_PI ) * _SpeedLinesTiling )));
